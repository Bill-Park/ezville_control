{
  "name": "MQTT2elfin EzVille Wallpad Controller with Python",
  "version": "0.0.1",
  "slug": "ezville_wallpad_mqtt_only",
  "url": "https://github.com/ktdo79/HAaddon",
  "description": "mqtt 통신을 활용한 EzVille 월패드 컨트롤러",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "DEBUG": false,
    "mqtt_log": true,
    "elfin_log": false,
    "save_unregistered_signal": false,
    "mqtt_server": "192.168.x.x",
    "mqtt_id": "id",
    "mqtt_password": "password",
    "elfin_server": "192.168.x.x",
    "elfin_id": "admin",
    "elfin_password": "elfin_password"
  },
  "schema": {
    "DEBUG": "bool",
    "mqtt_log": "bool",
    "elfin_log": "bool",
    "save_unregistered_signal": "bool",
    "mqtt_server": "str",
    "mqtt_id": "str",
    "mqtt_password": "str",
    "elfin_server": "str",
    "elfin_id": "str",
    "elfin_password": "str"
  }
}
